model Classified {
  id Int @id @default(autoincrement())
  views Int @default(0)
  slug String @unique
  vrm String
  title String?
  description String?
  year Int
  odoReading Int @default(0) @map("odo_reading")
  doors Int @default(2)
  seats Int @default(5)
  price Int @default(0) 
  makeId Int @map("make_id")
  modelId Int @map("model_id")
  modelVariantId Int @map("model_variant_id")
  
}
